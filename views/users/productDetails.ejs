<%- include('userSubHeader') %>


       <div class="popup" style="min-width: 300px; height: 70px; background-color: rgba(128, 128, 128, 0.459);
        position: fixed; z-index: 1000; right: 0; display: none; justify-content: center; align-items: center; color: white; border-radius: 10px; margin: 10px;">
                <span style=" font-size: 20px;">Product Added To Cart</span>
       </div> 

        <section class="mt-50 mb-50">
            <div class="container">
                <div class="row">
                    <div class="col-lg-9">
                        <div class="product-detail accordion-detail">
                            <div class="row mb-50">
                                <div class="col-md-6 col-sm-12 col-xs-12">
                                    <div class="detail-gallery">
                                        <span class="zoom-icon"><i class="fi-rs-search"></i></span>
                                        <!-- MAIN SLIDES -->
                                        <div class="product-image-slider">

                                            <% productdetails.images.forEach((items)=>{%>
                                            
                                                <figure class="border-radius-10">
                                                    <img src="<%= items %>" alt="product image">
                                                </figure>
                                            
                                           <% }) %>


                                           
                                        </div>
                                        <!-- THUMBNAILS -->
                                        <div class="slider-nav-thumbnails pl-15 pr-15">

                                            <% productdetails.images.forEach((images)=>{ %>
                                                <div><img src="<%= images %>" alt="product image"></div>

                                            <% }) %>
                                            <!-- <div><img src="/user/assets/imgs/shop/thumbnail-3.jpg" alt="product image"></div> -->
                                           
                                        </div>
                                    </div>
                                    <!-- End Gallery -->
                                    <div class="social-icons single-share">
                                        <ul class="text-grey-5 d-inline-block">
                                            <li class="social-instagram"><a href="https://www.instagram.com/_kvm_aquatics_/"><img src="/user/assets/imgs/theme/icons/icon-instagram.svg" alt=""></a></li>
                                        </ul>
                                    </div>
                                </div>




                                <div class="col-md-6 col-sm-12 col-xs-12">
                                    <div class="detail-info">
                                        <h2 class="title-detail amatic-sc-bold" style="font-size: 2.5rem;"> <%= productdetails.name  %> </h2>
                                        <div class="product-detail-rating">
                                           
                                            
                                        </div>
                                        <div class="clearfix product-price-cover">
                                            <div class="product-price primary-color float-left">
                                                <ins><span class="amatic-sc-bold" style="color: #b63f3f; font-size: 2.5rem;">₹<%= productdetails.price  %></span></ins>
                                                <ins><span class="old-price ml-15 gruppo-regular" style="font-size: 1.5rem;">₹ <%= productdetails.offerPrice  %></span></ins>
                                                <p style="margin-top: 3px; color: green; font-size: 15px;" class="gruppo-regular">Free Delivery</span>

                                            </div>
                                        </div>
                                        <div class="bt-1 border-color-1 mt-15 mb-15"></div>
                                        <div class="short-desc mb-30 gruppo-regular">
                                            <p><%= productdetails.description  %></p>
                                        </div>
                                        
                                        
                                        <div class="bt-1 border-color-1 mt-30 mb-30"></div>
                               <div class="detail-extralink">
    <div class="product-extra-link2">
        <% if (productdetails.quantity > 0) { %>
            <a 
                href="https://wa.me/918593025027?text=<%= encodeURIComponent(`I want this product: ${productdetails.name}. Is Available ?: ${productdetails.images[0]}`) %>" 
                target="_blank" 
                class="button w-100 amatic-sc-bold" 
                style="background-color: #b63f3f; font-size: 1.5rem; text-align: center; display: inline-block; padding: 0px 20px; color: white; text-decoration: none; border-radius: 5px;">
                ORDER NOW!
            </a>
        <% } else { %>
        <% } %>
        
    </div>
</div>


                                      
                                        
                                        
                                        
                                        <ul class="product-meta font-xs color-grey mt-50">
                                            <li class="gruppo-regular" style="font-size: 1rem;">STOCK :<span class="in-stock text-success ml-5"><%= productdetails.quantity  %></span></li>
                                        </ul>
                                    </div>
                                    <!-- Detail Info -->
                                
                             
                                </div>
                                





                            </div>
                           
                            
                            
                        </div>
                    </div>
                </div>
            </div>
        </section>
    </main>
    <footer class="main" style="background-color: #b63f3fb4;">
        <section class="section-padding footer-mid">
            <div class="container pt-15 pb-20">
                <div class="row justify-content-center text-center text-md-start">
                    <!-- About Section -->
                    <div class="col-lg-4 col-md-6 mb-4">
                        <div class="widget-about font-md">
                            <div class="logo logo-width-1">
                                <h3><p class="gruppo-regular" style="color: black; font-size: 2rem;">KVM<span style="color: #ffffff;"> AQUATICS</span></p></h3>
                            </div>
                            <h5 class="mt-20 mb-10 text-white gruppo-regular">Contact</h5>
                            <p class="text-white">
                                <p class="text-white gruppo-regular" style="font-size: 1.5rem;">+91 8593025027</p>
                            </p>
                           
                        </div>
                    </div>
                    <!-- About Links -->
                    <div class="col-lg-2 col-md-3 col-sm-6 mb-4">
                        <h5 class="widget-title text-white gruppo-regular" style="font-size: 1.5rem;">About</h5>
                        <ul class="footer-list list-unstyled">
                            <li><a href="/contact-us" class="text-white amatic-sc-bold" style="font-size: 1.3rem;">Contact Us</a></li>
                            <li><a href="/homeProduct" class="text-white amatic-sc-bold" style="font-size: 1.3rem;">Shop</a></li>
                        </ul>
                    </div>
                    <!-- Account Links -->
                    
                </div>
            </div>
        </section>
      </footer>
    
  
    <!-- Vendor JS-->
    <script src="/user/assets/js/vendor/modernizr-3.6.0.min.js"></script>
    <script src="/user/assets/js/vendor/jquery-3.6.0.min.js"></script>
    <script src="/user/assets/js/vendor/jquery-migrate-3.3.0.min.js"></script>
    <script src="/user/assets/js/vendor/bootstrap.bundle.min.js"></script>
    <script src="/user/assets/js/plugins/slick.js"></script>
    <script src="/user/assets/js/plugins/jquery.syotimer.min.js"></script>
    <script src="/user/assets/js/plugins/wow.js"></script>
    <script src="/user/assets/js/plugins/jquery-ui.js"></script>
    <script src="/user/assets/js/plugins/perfect-scrollbar.js"></script>
    <script src="/user/assets/js/plugins/magnific-popup.js"></script>
    <script src="/user/assets/js/plugins/select2.min.js"></script>
    <script src="/user/assets/js/plugins/waypoints.js"></script>
    <script src="/user/assets/js/plugins/counterup.js"></script>
    <script src="/user/assets/js/plugins/jquery.countdown.min.js"></script>
    <script src="/user/assets/js/plugins/images-loaded.js"></script>
    <script src="/user/assets/js/plugins/isotope.js"></script>
    <script src="/user/assets/js/plugins/scrollup.js"></script>
    <script src="/user/assets/js/plugins/jquery.vticker-min.js"></script>
    <script src="/user/assets/js/plugins/jquery.theia.sticky.js"></script>
    <script src="/user/assets/js/plugins/jquery.elevatezoom.js"></script>
    <!-- Template  JS -->
    <script src="/user/assets/js/main.js"></script>
    <script src="/user/assets/js/shop.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@10"></script>
</body>

</html>